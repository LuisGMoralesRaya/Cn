<script type="text/javascript" defer="defer">
      function getNUX_w23() {
        $.urlParam = function(name){
          var results = new RegExp('[\?&]' + name + '=([^&#]*)').exec(window.location.href);
          if (results==null) {
             return null;
          }
          return decodeURI(results[1]) || 0;
        }
        var out = $.urlParam('nux');
        if( out === "feb-23" ){
          setTimeout(function() {
            $('div.quick-links').removeClass('hidden');
          }, 1000);
        }else{
          setTimeout(function() {
            $('div.container_carousel').removeClass('hidden');
          }, 1000);
        }
      }
      document.addEventListener("DOMContentLoaded", function() {
        getNUX_w23();
      });
</script>