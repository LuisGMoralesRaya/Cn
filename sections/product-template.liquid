{% include 'settings_theme_instance' %}

{%- capture section_settings -%}
{
  "showShippingEstimator": {% if template == 'product.quick-view' or product.available == false %}false{% else %}{{ section.settings.show_shipping_estimator | json }}{% endif %},
  "showQuantitySelector": {{ section.settings.show_quantity_selector | json }},
  "showPaymentButton": {% if product.template_suffix == 'pre-order' %}false{% else %}{{ section.settings.show_payment_button | json }}{% endif %},
  "showInventoryQuantity": {% if product.template_suffix != 'pre-order' %}{{ section.settings.show_inventory_quantity | json }}{% else %}false{% endif %},
  "lowInventoryThreshold": {{ section.settings.low_inventory_threshold | json }},
  "galleryTransitionEffect": {{ section.settings.carousel_effect | json }},
  "enableImageZoom": {% if template == 'product.quick-view' %}false{% else %}{{ section.settings.enable_image_zoom | json }}{% endif %},
  "zoomEffect": {{ section.settings.zoom_effect | json }},
  "enableVideoLooping": {{ section.settings.enable_video_looping | json }},
  "productOptions": {{ product.options | json | escape }},
  "enableHistoryState": {% if template == 'product.quick-view' %}false{% else %}true{% endif %},
  "infoOverflowScroll": {% if template == 'product.quick-view' %}false{% else %}true{% endif %},
  "isQuickView": {% if template == 'product.quick-view' %}true{% else %}false{% endif %}
}
{%- endcapture -%}
{% style %}
  {% if product.tags contains '04-clc' or product.tags contains '01-clp' %}
    body.template-product .material_video,
    body.template-product .material_description,
    body.template-product .material_delivery_time{
      display:none !important;
    }
    div.material_description {
      margin-top: -10px !important;
      margin-bottom: 20px !important;
    }
  {% endif %}
{% endstyle %}
<style>
  html body.pplefileupload div.drift-bounding-box.drift-open{
    display: none !important;
  }
  span.pplr_preview_wrapper {
      display: block !important;
  }
  canvas#pplr_canvas {
      position: absolute !important;
      display: block !important;
      width: 100% !important;
      height: 100% !important;
      top: 0 !important;
      left: 0 !important;
  }
  label.pplrlabel {
    margin-right: 10px !important;
  }
  div.product-personalizer {
    position: relative !important;
    z-index: 1 !important;
  }
  .pplr-btn.button.ptc_button{
    background: #1e2d7d !important;
    color: #fff !important;
    text-shadow: 0 0 10px #00adef !important;
    border-radius: 5px !important;
    transition: all 0.3s !important;
  }

  .pplr-btn.button.ptc_button:hover {
    background: #f0297c !important;
    text-shadow: 0 0 10px #da0000 !important;
  }

</style>





<section data-section-id="{{ section.id }}" data-section-type="product" data-section-settings='{{ section_settings }}'>
  {%- if template != 'product.quick-view' -%}
    <div class="container container--flush">
      <div class="page__sub-header">
        <nav aria-label="{{ 'general.breadcrumb.title' | t }}" class="breadcrumb">
          <ol class="breadcrumb__list" role="list">
            <li class="breadcrumb__item">
              <a class="breadcrumb__link link" href="{{ routes.root_url }}">{{ 'general.breadcrumb.home' | t }}</a> {%- render 'icon', icon: 'arrow-right' -%}
            </li>

            <li class="breadcrumb__item">
              {%- if collection -%}
                <a class="breadcrumb__link link" href="{{ collection.url }}">{{ collection.title }}</a> {%- render 'icon', icon: 'arrow-right' -%}
              {%- else -%}
                <a class="breadcrumb__link link" href="{{ routes.all_products_collection_url }}">{{- 'collection.general.all_products' | t -}}</a> {%- render 'icon', icon: 'arrow-right' -%}
              {%- endif -%}
            </li>

            <li class="breadcrumb__item">
              <span class="breadcrumb__link" aria-current="page">{{ product.title | truncate: 40 }}</span>
            </li>
          </ol>
        </nav>

        {%- if collection.previous_product or collection.next_product -%}
          <div class="page__navigation">
            {%- if collection.previous_product -%}
              <span class="page__navigation-item page__navigation-item--prev">
                <a href="{{ collection.previous_product.url }}" class="link" rel="prev">{%- render 'icon', icon: 'arrow-left' -%} {{- 'product.general.previous_product' | t -}}</a>
              </span>
            {%- endif -%}

            {%- if collection.next_product -%}
              <span class="page__navigation-item page__navigation-item--next">
                <a href="{{ collection.next_product.url }}" class="link" rel="next">{{- 'product.general.next_product' | t -}} {%- render 'icon', icon: 'arrow-right' -%}</a>
              </span>
            {%- endif -%}
          </div>
        {%- endif -%}
      </div>

      <div class="product-block-list product-block-list--{{ section.settings.image_size }}">
        <div class="product-block-list__wrapper">
          {%- if product.media.size > 0 -%}
            <div class="product-block-list__item product-block-list__item--gallery">
              {% render 'product-gallery' %}
            </div>
          {%- endif -%}

          <div class="product-block-list__item product-block-list__item--info">
            {% render 'product-info', prom_v1: prom_v1, prom_v2: prom_v2, prom_v3: prom_v3 %}
          </div>
          
          {%- if product.tags contains '04-clc' or product.tags contains '01-clp'  -%}
          <div class="product-block-list__item product-block-list__item--description">
            <div class="card">
              <div class="card__header">
                <h2 class="card__title heading h3">{{ 'product.general.description' | t }}</h2>
              </div>
              <div class="card__section cl_description_icons">
                <div class="material_description" id="can_d">
                  <img loading="lazy" width="auto" height="100%" style="display:block;margin: 0 auto 10px auto;width: auto !important;height: auto !important;" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/d_can.jpg?v=1654029746"/>
                  <div id="cl_cc" style="line-height: 1.2em !important; font-size: 15px; text-align: justify; width: 90%;">
                    <div class="fixer_cl">
                      <small class="heading h4">Tipo de perfil</small>
                      <br><small><b>Selecciona espejeado</b> para una impresión en espejo de la imagen principal por los lados y las partes superior e inferior.</small>
                      <br><small><b>Selecciona color</b> para un color sólido por los lados y las partes superior e inferior.</small>
                    </div>
                  </div>
                  <img loading="lazy" width="auto" height="100%" style="display:block;margin: 0 auto 10px auto;width: auto !important;height: auto !important;" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/pok.png?v=1619555761">
                  Nuestros Canvas son fabricados con materiales premium y llegan listos para colgarse en cualquier pared. Utilizamos lienzo de primera calidad impresos con tecnología latex, montados en bastidores de madera de tala sustentable con calidad de exportación. Con estas tecnologías y materiales se logra la mejor resolución, los colores más vibrantes y la mejor durabilidad disponible en el mercado.
                </div>
                <div class="material_description" id="fra_d">
                  Cuadro impreso con tecnología latex, enmarcado, protegido por el frente con acrílico premium de seguridad y por el reverso con una placa de MDF. Por detrás incluye un caimán para que lo recibas listo para colgarse.
                  <img loading="lazy" width="auto" height="100%" style="display:block;margin: 0 auto 10px auto;width: auto !important;height: auto !important;" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/Because_-_diferenciadores_-_Enmarcados.jpg?v=1675369333"/>
                </div>
                <div class="material_description" id="pos_d">
                  Póster de alta resolución impreso con tecnología latex, ecológicamente sustentable y de larga duración.
                </div>
                <div class="material_description" id="acr_d">
                  Nuestros Acrílicos son impresos en directo con tecnología Latex, lo que logra la mejor resolución y los colores más vibrantes posibles. Por la parte posterior, están montados sobre un bastidor de madera de tala sustentable. Llegan listos para colgar en cualquier pared.
                </div>
                <div class="material_description" id="alu_d">
                  Nuestros Aluminios son impresos en directo sobre placas de Aluminio Dibond con tecnología UV, lo que logra la mejor resolución y los colores más vibrantes posibles. Por la parte posterior, están montados sobre un bastidor de Aluminio. Llegan listos para colgar en cualquier pared.
                </div>
                <div>
                  <img loading="lazy" width="auto" height="100%" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/iconbox.svg?v=1613235246">Amas tus cuadros o te regresamos tu dinero.
                </div>

                <div>
                  <img loading="lazy" width="auto" height="100%" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/envios_3f0b4a8f-b392-4256-9d31-2be4313e9194.svg?v=1613235246">Envío gratis a todo México
                </div>

                <div>
                  <img loading="lazy" width="auto" height="100%" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/compra-segura.svg?v=1613235246">Compra Segura
                </div>
              </div>
            </div>
          </div>
          {%- else -%}
            {%- if product.description != blank -%}
              <div class="product-block-list__item product-block-list__item--description" {% for t in product.tags %} {% if t contains 'giftcard' %} style="display:none !important;" {% endif %} {% endfor %}>
                <div class="card">

                  {%- if section.settings.content_display_mode == 'collapse_all' -%}
                    <button class="card__collapsible-button" data-action="toggle-collapsible" aria-expanded="false" aria-controls="product-description">
                      <span class="card__title heading h3">{{ 'product.general.description' | t }}</span>
                      <span class="plus-button plus-button--large"></span>
                    </button>

                    <div id="product-description" class="card__collapsible">
                      <div class="card__collapsible-content">
                        <div class="rte text--pull">
                          {{ product.description | remove: 'data-section-type="product"' }}
                        </div>
                      </div>
                    </div>
                  {%- else -%}

                    <div class="card__header">

                      <h2 class="card__title heading h3">{{ 'product.general.description' | t }}</h2>
                    </div>

                    {%- if section.settings.content_display_mode == 'show_all_and_expand_description' -%}
                      
                      <div class="card__section cl_description_icons">
                        {%- if product.title contains 'Kit de Collage' -%}
                        {% if product.tags contains "custom" %}
                          <img loading="lazy" style="display:block;margin: 0 auto 10px auto;width: auto !important;height: auto !important;" width="auto" height="100%" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/Because-Kits-personalizados-Diferenciadores.png?v=1693342555"/>
                          <br>
                          <p>{{ product.title }}</p>
                          {{ product.description }}
                        {% else %}
                          <img loading="lazy" style="display:block;margin: 0 auto 10px auto;width: auto !important;height: auto !important;" width="auto" height="100%" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/Kits_Collage_Because-Diferenciadores_b7ba1cc5-9fad-40ec-97c2-1f9db25e2568.jpg?v=1683922494"/>
                          <br>
                          <p>{{ product.title }}</p>
                          <p>Nuestros Kits de collage consisten de 50 tarjetas de 10 x 15 cm, todas con imágenes distintas. Estan impresas en papel fotográfico semibrillante premium de 300g, más gruesas que una fotografía normal para que no pierdan su forma con el tiempo. Incluyen cinta doble cara especial para pegar todas las tarjetas donde quieras.</p>
                        {% endif %}   
                        <br>
                        
                        {%- else -%}
                        <p>{% unless product.title contains "Tarjeta de Regalo" %}Cuadro decorativo{% endunless %} {{product.title}}</p>
                        <p>Todos nuestros cuadros son fabricados con materiales premium y llegan lístos para colgarse en cualquier pared. Los Canvas, Acrílicos y Posters son impresos con tecnología latex, mientras que los Aluminios son impresos con tecnología UV. Con estas tecnologías se logran las mejores resoluciones y los colores más vibrantes del mercado.</p>
                        <p>Los posters no incluyen adhesivos ni marcos.</p>
                        {%- endif -%}                      

                        <div>
                        <img loading="lazy" width="auto" height="100%" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/iconbox.svg?v=1613235246">Amas tus cuadros o te regresamos tu dinero.
                        </div>

                        <div>
                        <img loading="lazy" width="auto" height="100%" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/envios_3f0b4a8f-b392-4256-9d31-2be4313e9194.svg?v=1613235246">Envío gratis a todo México
                        </div>

                        <div>
                        <img loading="lazy" width="auto" height="100%" src="https://cdn.shopify.com/s/files/1/0258/6276/6677/files/compra-segura.svg?v=1613235246">Compra Segura
                        </div><br>
                        <!--<p>¿Necesitas ayuda? <a href="/pages/preguntas-frecuentes" alt="Preguntas Frecuentes" target="_blank" class="cl_product_help">Ver preguntas frecuentes</a> o <a href="/pages/ayuda" alt="Centro de Ayuda" target="_blank" class="cl_product_help">visita nuestro centro de ayuda</a>. </p>-->


                       <!-- <div class="rte text--pull">
                          {{ product.description | remove: 'data-section-type="product"' }}
                        </div>-->
                      </div>
                    {%- else -%}
                      <div class="card__section expandable-content" aria-expanded="false">
                        <div class="rte text--pull">
                          {{ product.description | remove: 'data-section-type="product"' }}
                        </div>

                        <button class="expandable-content__toggle">
                          <span class="expandable-content__toggle-icon"></span>
                          <span class="expandable-content__toggle-text" data-view-more="{{ 'product.general.view_more' | t | escape }}" data-view-less="{{ 'product.general.view_less' | t | escape }}">{{- 'product.general.view_more' | t -}}</span>
                        </button>
                      </div>
                    {%- endif -%}
                  {%- endif -%}
                </div>
              </div>
            {%- endif -%}
          {%- endif -%}
          
          <div class="product-block-list__item product-block-list__item--trust">
            <div class="card">
              <div class="card__header card__header--flex">
                <h2 class="card__title heading h3">Paga con confianza</h2><svg focusable="false" class="icon icon--lock" viewBox="0 0 18 24" role="presentation">
                <g fill="none" fill-rule="evenodd" stroke="currentColor" stroke-linecap="square" stroke-width="2">
